{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "airbnb",
    "rules":{
        "no-console": "warn",
        "no-unused-vars": "warn",
        "no-param-reassign": [2, {"props":false}],
        "func-names": 0,
        "comma-dangle" : 0,
        "max-len" : 0,
        "class-methods-use-this" : 0,
        "no-undef" : 0,
        "no-await-in-loop" : 0,
        "no-plusplus" : 0,
        "no-restricted-syntax" : 0,
        "guard-for-in" :0 ,
        "no-loop-func" :0,
        "radix": 0,
        "prefer-spread":0,
        "prefer-destructuring":0,
        "no-use-before-define":0,
        "no-mixed-operators":0,
        "new-cap":0,
        "array-callback-return":0,
        "consistent-return":0,
        "no-underscore-dangle":0,
        "no-useless-concat":0,
        "linebreak-style":0
       
    }
}
